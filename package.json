{
  "name": "jsdom-mount",
  "version": "0.1.0",
  "description": "Helper for mounting Dom in unit tests",
  "main": "index.js",
  "scripts": {
    "build": "babel src/index.js --out-file index.js",
    "test": "mocha --compilers js:babel-core/register \"test/**/*.js\"",
    "test:tape": "tape -r babel-register test/**/*.js | tap-spec"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb/base",
    "rules": {
      "max-len": 0
    }
  },
  "keywords": [
    "test",
    "jsdom",
    "unit",
    "dataset",
    "polyfill"
  ],
  "author": "ColbyCheeZe",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^6.2.0",
    "mocha": "^2.4.5"
  },
  "dependencies": {
    "jsdom": "^8.2.0"
  }
}
